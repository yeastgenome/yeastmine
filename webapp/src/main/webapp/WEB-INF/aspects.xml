<aspects>

	<aspect name="Genome">
		<subtitle>Genome annotation</subtitle>
		<intro-text> Yeast genome features, domains, coordinates, sequences,
			homology information. </intro-text>
		<tile-name>model/genomics.jsp</tile-name>
		<tile-name>model/proteins.jsp</tile-name>
		<aspect-source name="SGD" url="http://www.yeastgenome.org" />
	</aspect>


	<aspect name="Proteins">
		<subtitle>Protein data</subtitle>
		<icon-image>model/images/proteins.png</icon-image>
		<large-image>model/images/proteins.png</large-image>
		<intro-text>
			Protein annotation from SGD.
		</intro-text>
		<tile-name>model/proteins.jsp</tile-name>
		<aspect-source name="SGD" url="http://www.yeastgenome.org" />

	</aspect>

	<aspect name="Function">
		<subtitle>GO, Pathways.</subtitle>
		<intro-text> Pathways and Gene Ontology (GO) annotations. </intro-text>
		<tile-name>model/GO.jsp</tile-name>
		<tile-name>model/pathways.jsp</tile-name>
		<aspect-source name="Gene Ontology Consortium" url="http://www.geneontology.org/" />
		<aspect-source name="SGD pathway database" url="http://pathway.yeastgenome.org/" />
	</aspect>

	<aspect name="Phenotypes">
		<subtitle>Phenotypes</subtitle>
		<intro-text> Phenotype data</intro-text>
		<tile-name>model/phenotypes.jsp</tile-name>
		<aspect-source name="SGD" url="http://www.yeastgenome.org" />
	</aspect>

	<aspect name="Interactions">
		<subtitle>Interactions</subtitle>
		<intro-text>Interactions from BioGRID.</intro-text>
		<tile-name>model/interactions.jsp</tile-name>
		<aspect-source name="BioGRID" url="http://www.thebiogrid.org/" />
	</aspect>

	<aspect name="Literature">
		<subtitle>Literature</subtitle>
		<intro-text> A mapping between genes and publications for
			&lt;i&gt;S.cerevisiae&lt;/i&gt;.</intro-text>
		<tile-name>model/literature.jsp</tile-name>
		<aspect-source name="NCBI" url="http://www.ncbi.nlm.nih.gov/" />
		<aspect-source name="SGD" url="http://www.yeastgenome.org" />
	</aspect>

	<aspect name="Expression">
		<subtitle>Gene Expression</subtitle>
		<intro-text> &lt;p&gt; Gene expression data for &lt;i&gt;S. cerevisiae
			&lt;/i&gt;.</intro-text>
		<tile-name>model/microarray.jsp</tile-name>
		<aspect-source name="SPELL" url="http://spell.yeastgenome.org/" />
	</aspect>
	
		<aspect name="Disease">
		<subtitle>Curated Disease Data</subtitle>
		<intro-text> &lt;p&gt; Curated Disease data for &lt;i&gt;S. cerevisiae
			&lt;/i&gt;.</intro-text>
		<tile-name>model/microarray.jsp</tile-name>
		<aspect-source name="SPELL" url="http://www.yeastgenome.org/" />
	</aspect>

	<aspect name="Regulation">
		<subtitle>Regulation</subtitle>
		<intro-text> &lt;p&gt; Gene regulation data for &lt;i&gt;S. cerevisiae
			&lt;/i&gt;. </intro-text>
		<aspect-source name="Regulation" url="http://yeastract.com/" />
	</aspect>

	<aspect name="Homology">
		<subtitle>Homology</subtitle>
		<intro-text> &lt;p&gt; Homology data for &lt;i&gt;S. cerevisiae
			&lt;/i&gt;. </intro-text>
		<aspect-source name="Homology" url="http://yeastmine.yeastgenome.org/" />
	</aspect>

	<aspect name="Downloads">
		<subtitle>Downloads</subtitle>
		<intro-text> &lt;p&gt; Download data for &lt;i&gt;S. cerevisiae
			&lt;/i&gt;. </intro-text>
		<aspect-source name="Downloads" url="http://yeastmine.yeastgenome.org/" />
	</aspect>


</aspects>
