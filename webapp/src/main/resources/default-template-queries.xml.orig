<?xml version="1.0"?>
<userprofile>
	<items />
	<bags />
	<queries>
	</queries>

	<template-queries>
<template name="A_IdentifierSynonym_Object" title="Identifier or synonym --&gt; Database object." longDescription="Find an entry in the database using any identifier or synonym." comment="This is the template that the quick search runs.  If the name is changed it also has to be changed for the quick search configuration - Rachel. Removed tags so no longer displayed - just used for quick search now.  Rachel.">
  <query name="A_IdentifierSynonym_Object" model="genomic" view="Synonym.subject.primaryIdentifier Synonym.subject.secondaryIdentifier Synonym.subject.symbol Synonym.value Synonym.subject.sgdAlias Synonym.subject.organism.name" longDescription="Find an entry in the database using any identifier or synonym." sortOrder="Synonym.subject.primaryIdentifier asc">
    <node path="Synonym" type="Synonym">
    </node>
    <node path="Synonym.value" type="String">
      <constraint op="=" value="act1" description="" identifier="Synonym.value.1" editable="true" code="A">
      </constraint>
    </node>
  </query>
</template>
<template name="AllGene_ChromosomalFeatures" title="All genes in organism --&gt; All Chromosomal Features" longDescription="List all chromosomal features for the selected organism. Features include Uncharacterized and Verified ORFs, pseudogenes, transposable elements and transposable element genes, RNAs, telomeric regions, centromeres, ARS, genes Not in Systematic Sequence of S228C, and genes Not Physically Mapped." comment="">
  <query name="AllGene_ChromosomalFeatures" model="genomic" view="LocatedSequenceFeature.primaryIdentifier LocatedSequenceFeature.featureType LocatedSequenceFeature.secondaryIdentifier LocatedSequenceFeature.description LocatedSequenceFeature.sgdAlias" longDescription="List all chromosomal features for the selected organism. Features include Uncharacterized and Verified ORFs, pseudogenes, transposable elements and transposable element genes, RNAs, telomeric regions, centromeres, ARS, genes Not in Systematic Sequence of S228C, and genes Not Physically Mapped." sortOrder="LocatedSequenceFeature.primaryIdentifier asc" constraintLogic="(A or B or C or D or E or F or G or H or I or J or K or L or M or N or O or P or Q or R or S) and T and U and (V or W)">
    <node path="LocatedSequenceFeature" type="LocatedSequenceFeature">
    </node>
    <node path="LocatedSequenceFeature.featureType" type="String">
      <constraint op="=" value="ORF" description="" identifier="" code="A" extraValue="">
      </constraint>
      <constraint op="=" value="ARS" description="" identifier="" code="B" extraValue="">
      </constraint>
      <constraint op="=" value="X_element_combinatorial_repeats" description="" identifier="" code="C" extraValue="">
      </constraint>
      <constraint op="=" value="X_element_core_sequence" description="" identifier="" code="D" extraValue="">
      </constraint>
      <constraint op="=" value="Y'_element" description="" identifier="" code="E" extraValue="">
      </constraint>
      <constraint op="=" value="centromere" description="" identifier="" code="F" extraValue="">
      </constraint>
      <constraint op="=" value="long_terminal_repeat" description="" identifier="" code="G" extraValue="">
      </constraint>
      <constraint op="=" value="ncRNA" description="" identifier="" code="H" extraValue="">
      </constraint>
      <constraint op="=" value="not in systematic sequence of S288C" description="" identifier="" code="I" extraValue="">
      </constraint>
      <constraint op="=" value="not physically mapped" description="" identifier="" code="J" extraValue="">
      </constraint>
      <constraint op="=" value="pseudogene" description="" identifier="" code="K" extraValue="">
      </constraint>
      <constraint op="=" value="rRNA" description="" identifier="" code="L" extraValue="">
      </constraint>
      <constraint op="=" value="retrotransposon" description="" identifier="" code="M" extraValue="">
      </constraint>
      <constraint op="=" value="snRNA" description="" identifier="" code="N" extraValue="">
      </constraint>
      <constraint op="=" value="snoRNA" description="" identifier="" code="O" extraValue="">
      </constraint>
      <constraint op="=" value="tRNA" description="" identifier="" code="P" extraValue="">
      </constraint>
      <constraint op="=" value="telomere" description="" identifier="" code="Q" extraValue="">
      </constraint>
      <constraint op="=" value="telomeric_repeat" description="" identifier="" code="R" extraValue="">
      </constraint>
      <constraint op="=" value="transposable_element_gene" description="" identifier="" code="S" extraValue="">
      </constraint>
    </node>
    <node path="LocatedSequenceFeature.organism" type="Organism">
    </node>
    <node path="LocatedSequenceFeature.organism.name" type="String">
      <constraint op="=" value="Saccharomyces cerevisiae" description="" identifier="" editable="true" code="T" extraValue="">
      </constraint>
    </node>
    <node path="LocatedSequenceFeature.status" type="String">
      <constraint op="=" value="Active" description="" identifier="" code="U" extraValue="">
      </constraint>
    </node>
    <node path="LocatedSequenceFeature.qualifier" type="String">
      <constraint op="!=" value="Dubious" description="" identifier="" code="V">
      </constraint>
      <constraint op="IS NULL" value="null" description="" identifier="" code="W">
      </constraint>
    </node>
  </query>
</template>
<template name="AllGenes_Topic_Literature" title="All genes in organism --&gt;Topic--&gt;Literature" longDescription="Show all genes associated with a certain topic." comment="">
  <query name="AllGenes_Topic_Literature" model="genomic" view="Gene.primaryIdentifier Gene.secondaryIdentifier Gene.symbol Gene.name Gene.sgdAlias Gene.organism.shortName Gene.publicationAnnotations.publication.pubMedId Gene.publicationAnnotations.publication.citation Gene.publicationAnnotations.publication.title Gene.publicationAnnotations.publication.journal Gene.publicationAnnotations.literatureTopics.name" longDescription="Show all genes associated with a certain topic." sortOrder="Gene.primaryIdentifier asc" constraintLogic="A and B">
    <node path="Gene" type="Gene">
    </node>
    <node path="Gene.publicationAnnotations" type="PublicationAnnotation">
    </node>
    <node path="Gene.publicationAnnotations.literatureTopics" type="LiteratureTopic">
    </node>
    <node path="Gene.publicationAnnotations.literatureTopics.name" type="String">
      <constraint op="=" value="Industrial Applications" description="" identifier="" editable="true" code="A">
      </constraint>
    </node>
    <node path="Gene.organism" type="Organism">
    </node>
    <node path="Gene.organism.shortName" type="String">
      <constraint op="=" value="S.cerevisiae" description="" identifier="" code="B">
      </constraint>
    </node>
  </query>
</template>
<template name="ChromosomeRegion_AllGenes" title="Chromosomal Region --&gt; All genes" longDescription="List all genes located within the specified chromosome and coordinate range. Genes include Uncharacterized and Verified ORFs, pseudogenes, transposable element genes, and RNAs." comment="">
  <query name="ChromosomeRegion_AllGenes" model="genomic" view="Gene.chromosome.primaryIdentifier Gene.primaryIdentifier Gene.secondaryIdentifier Gene.featureType Gene.symbol Gene.name Gene.sgdAlias Gene.organism.shortName Gene.chromosomeLocation.start Gene.chromosomeLocation.end Gene.chromosomeLocation.strand Gene.description" longDescription="List all genes located within the specified chromosome and coordinate range. Genes include Uncharacterized and Verified ORFs, pseudogenes, transposable element genes, and RNAs." sortOrder="Gene.primaryIdentifier asc" constraintLogic="(A or B) and C and D and E and (F or G) and I">
    <node path="Gene" type="Gene">
    </node>
    <node path="Gene.status" type="String">
      <constraint op="=" value="Active" description="" identifier="" code="A" extraValue="">
      </constraint>
      <constraint op="IS NULL" value="null" description="" identifier="" code="B">
      </constraint>
    </node>
    <node path="Gene.chromosome" type="Chromosome">
    </node>
    <node path="Gene.chromosome.primaryIdentifier" type="String">
      <constraint op="=" value="14" description="" identifier="" editable="true" code="C" extraValue="">
      </constraint>
    </node>
    <node path="Gene.chromosomeLocation" type="Location">
    </node>
    <node path="Gene.chromosomeLocation.start" type="Integer">
      <constraint op="&gt;=" value="1" description="" identifier="" editable="true" code="D">
      </constraint>
    </node>
    <node path="Gene.chromosomeLocation.end" type="Integer">
      <constraint op="&lt;=" value="20000" description="" identifier="" editable="true" code="E">
      </constraint>
    </node>
    <node path="Gene.qualifier" type="String">
      <constraint op="!=" value="Dubious" description="" identifier="" code="F">
      </constraint>
      <constraint op="IS NULL" value="null" description="" identifier="" code="G">
      </constraint>
    </node>
    <node path="Gene.featureType" type="String">
      <constraint op="!=" value="not in systematic sequence of S288C" description="" identifier="" code="I">
      </constraint>
    </node>
  </query>
</template>
<template name="Chromosome_Gene" title="Chromosome --&gt; All genes." longDescription="List all genes located on the selected chromosome. Genes include Uncharacterized and Verified ORFs, pseudogenes, transposable element genes, RNAs, and genes Not in Systematic Sequence of S228C." comment="">
  <query name="Chromosome_Gene" model="genomic" view="Gene.chromosome.primaryIdentifier Gene.primaryIdentifier Gene.secondaryIdentifier Gene.featureType Gene.symbol Gene.sgdAlias Gene.qualifier Gene.chromosomeLocation.start Gene.chromosomeLocation.end Gene.chromosomeLocation.strand Gene.description" longDescription="List all genes located on the selected chromosome. Genes include Uncharacterized and Verified ORFs, pseudogenes, transposable element genes, RNAs, and genes Not in Systematic Sequence of S228C." sortOrder="Gene.primaryIdentifier asc" constraintLogic="A and (B or C) and (D or E)">
    <node path="Gene" type="Gene">
    </node>
    <node path="Gene.chromosome" type="Chromosome">
    </node>
    <node path="Gene.chromosome.primaryIdentifier" type="String">
      <constraint op="=" value="3" description="Show genes located on chromosome" identifier="chromosome.primaryIdentifier" editable="true" code="A">
      </constraint>
    </node>
    <node path="Gene.organism" type="Organism">
    </node>
    <node path="Gene.status" type="String">
      <constraint op="=" value="Active" description="" identifier="" code="B" extraValue="">
      </constraint>
      <constraint op="IS NULL" value="null" description="" identifier="" code="C">
      </constraint>
    </node>
    <node path="Gene.qualifier" type="String">
      <constraint op="!=" value="Dubious" description="" identifier="" code="D">
      </constraint>
      <constraint op="IS NULL" value="null" description="" identifier="" code="E">
      </constraint>
    </node>
  </query>
</template>
<template name="Chromosome_Gene_FeatureType" title="Chromosome--&gt;Genes of a selected Feature Type" longDescription="List genes of a selected Feature Type located on the selected chromosome. Available Feature Types include Uncharacterized and Verified ORFs, pseudogenes, transposable element genes, RNAs, and genes Not in Systematic Sequence of S228C." comment="">
  <query name="Chromosome_Gene_FeatureType" model="genomic" view="Gene.chromosome.primaryIdentifier Gene.primaryIdentifier Gene.secondaryIdentifier Gene.featureType Gene.symbol Gene.name Gene.organism.shortName Gene.description" longDescription="List genes of a selected Feature Type located on the selected chromosome. Available Feature Types include Uncharacterized and Verified ORFs, pseudogenes, transposable element genes, RNAs, and genes Not in Systematic Sequence of S228C." sortOrder="Gene.featureType asc" constraintLogic="A and B and (C or D) and (E or F)">
    <node path="Gene" type="Gene">
    </node>
    <node path="Gene.featureType" type="String">
      <constraint op="=" value="ORF" description="" identifier="" editable="true" code="A">
      </constraint>
    </node>
    <node path="Gene.chromosome" type="Chromosome">
    </node>
    <node path="Gene.chromosome.primaryIdentifier" type="String">
      <constraint op="=" value="1" description="" identifier="" editable="true" code="B">
      </constraint>
    </node>
    <node path="Gene.qualifier" type="String">
      <constraint op="!=" value="Dubious" description="" identifier="" code="C">
      </constraint>
      <constraint op="IS NULL" value="null" description="" identifier="" code="D">
      </constraint>
    </node>
    <node path="Gene.status" type="String">
      <constraint op="=" value="Active" description="" identifier="" code="E">
      </constraint>
      <constraint op="IS NULL" value="null" description="" identifier="" code="F">
      </constraint>
    </node>
  </query>
</template>
<template name="Chromosome_IntergenicSequence" title="Chromosome--&gt;All intergenic regions." longDescription="For a specified chromosome show all intergenic regions with their chromosomal locations and lengths." comment="">
  <query name="Chromosome_IntergenicSequence" model="genomic" view="IntergenicRegion.primaryIdentifier IntergenicRegion.length IntergenicRegion.chromosomeLocation.object.primaryIdentifier IntergenicRegion.chromosomeLocation.start IntergenicRegion.chromosomeLocation.end IntergenicRegion.chromosomeLocation.strand IntergenicRegion:adjacentGenes.primaryIdentifier IntergenicRegion:adjacentGenes.secondaryIdentifier IntergenicRegion:adjacentGenes.symbol" longDescription="For a specified chromosome show all intergenic regions with their chromosomal locations and lengths." sortOrder="IntergenicRegion.primaryIdentifier asc" constraintLogic="A and B and C and D">
    <pathDescription pathString="IntergenicRegion.evidence" description="Dataset">
    </pathDescription>
    <pathDescription pathString="IntergenicRegion.chromosomeLocation.object" description="Chromosome">
    </pathDescription>
    <node path="IntergenicRegion" type="IntergenicRegion">
    </node>
    <node path="IntergenicRegion.organism" type="Organism">
    </node>
    <node path="IntergenicRegion.organism.shortName" type="String">
      <constraint op="=" value="S.cerevisiae" description="Select an organism:" identifier="" editable="true" code="A">
      </constraint>
    </node>
    <node path="IntergenicRegion.chromosomeLocation" type="Location">
    </node>
    <node path="IntergenicRegion.chromosomeLocation.object" type="Chromosome">
    </node>
    <node path="IntergenicRegion.chromosomeLocation.object.primaryIdentifier" type="String">
      <constraint op="=" value="1" description="Show intergenic regions for chromosome:" identifier="" editable="true" code="B">
      </constraint>
    </node>
    <node path="IntergenicRegion.length" type="Integer">
      <constraint op="&lt;=" value="10000" description="" identifier="" editable="true" code="C">
      </constraint>
    </node>
    <node path="IntergenicRegion:adjacentGenes" type="Gene">
    </node>
    <node path="IntergenicRegion:adjacentGenes.featureType" type="String">
      <constraint op="!=" value="not in systematic sequence of S288C" description="" identifier="" code="D">
      </constraint>
    </node>
  </query>
</template>
<template name="Chromosome_Telomeres" title="Chromosome--&gt;Telomeric Regions" longDescription="List telomeres and telomeric regions on the selected chromosome." comment="">
  <query name="Chromosome_Telomeres" model="genomic" view="Telomere.chromosome.primaryIdentifier Telomere.primaryIdentifier Telomere.secondaryIdentifier Telomere.featureType Telomere.organism.shortName Telomere.chromosomeLocation.start Telomere.chromosomeLocation.end Telomere.chromosomeLocation.strand Telomere.description" longDescription="List telomeres and telomeric regions on the selected chromosome." sortOrder="Telomere.primaryIdentifier asc">
    <node path="Telomere" type="Telomere">
    </node>
    <node path="Telomere.chromosome" type="Chromosome">
    </node>
    <node path="Telomere.chromosome.primaryIdentifier" type="String">
      <constraint op="=" value="1" description="" identifier="" editable="true" code="A">
      </constraint>
    </node>
  </query>
</template>
<template name="FeatureType_Gene" title="FeatureType--&gt;Genes" longDescription="List genes of a selected Feature Type. Available Feature Types include Uncharacterized and Verified ORFs, pseudogenes, transposable element genes, RNAs, and genes Not in Systematic Sequence of S228C." comment="">
  <query name="FeatureType_Gene" model="genomic" view="Gene:chromosome.primaryIdentifier Gene.primaryIdentifier Gene.secondaryIdentifier Gene.featureType Gene.name Gene.sgdAlias Gene.organism.shortName Gene:chromosomeLocation.start Gene:chromosomeLocation.end Gene:chromosomeLocation.strand Gene.description" longDescription="List genes of a selected Feature Type. Available Feature Types include Uncharacterized and Verified ORFs, pseudogenes, transposable element genes, RNAs, and genes Not in Systematic Sequence of S228C." sortOrder="Gene.primaryIdentifier asc" constraintLogic="(A or B) and C and (D or E)">
    <node path="Gene" type="Gene">
    </node>
    <node path="Gene.status" type="String">
      <constraint op="=" value="Active" description="" identifier="" code="A" extraValue="">
      </constraint>
      <constraint op="IS NULL" value="null" description="" identifier="" code="B">
      </constraint>
    </node>
    <node path="Gene.featureType" type="String">
      <constraint op="=" value="ORF" description="" identifier="" editable="true" code="C" extraValue="">
      </constraint>
    </node>
    <node path="Gene.symbol" type="String">
    </node>
    <node path="Gene.qualifier" type="String">
      <constraint op="!=" value="Dubious" description="" identifier="" code="D">
      </constraint>
      <constraint op="IS NULL" value="null" description="" identifier="" code="E">
      </constraint>
    </node>
    <node path="Gene:chromosome" type="Chromosome">
    </node>
    <node path="Gene:chromosomeLocation" type="Location">
    </node>
  </query>
</template>
<template name="GOSlimTerm_FeatureType_Gene" title="GO SlimTerm--&gt;Genes of a selected Feature Type" longDescription="List genes of a selected Feature Type that are annotated to selected GO Slim term and to GO terms that are children of that selected GO Slim Term. Only manually curated and high-throughput GO annotations are included. Available Feature Types include Uncharacterized and Verified ORFs, pseudogenes, transposable element genes, RNAs, and genes Not in Systematic Sequence of S228C." comment="">
  <query name="GOSlimTerm_FeatureType_Gene" model="genomic" view="Gene.primaryIdentifier Gene.secondaryIdentifier Gene.symbol Gene.name Gene.goAnnotation.ontologyTerm.identifier Gene.goAnnotation.ontologyTerm.parents.identifier Gene.goAnnotation.ontologyTerm.name Gene.goAnnotation.goEvidenceCodes.code Gene.organism.shortName Gene.goAnnotation.goEvidenceCodes.annotType Gene.qualifier" longDescription="List genes of a selected Feature Type that are annotated to selected GO Slim term and to GO terms that are children of that selected GO Slim Term. Only manually curated and high-throughput GO annotations are included. Available Feature Types include Uncharacterized and Verified ORFs, pseudogenes, transposable element genes, RNAs, and genes Not in Systematic Sequence of S228C." sortOrder="Gene.primaryIdentifier asc" constraintLogic="A and B and (C or D) and (E or F) and (G or H) and (I or J)">
    <node path="Gene" type="Gene">
    </node>
    <node path="Gene.goAnnotation" type="GOAnnotation">
    </node>
    <node path="Gene.goAnnotation.ontologyTerm" type="OntologyTerm">
    </node>
    <node path="Gene.goAnnotation.ontologyTerm.parents" type="GOSlimTerm">
    </node>
    <node path="Gene.goAnnotation.ontologyTerm.parents.name" type="String">
      <constraint op="=" value="DNA binding" description="" identifier="" editable="true" code="A">
      </constraint>
    </node>
    <node path="Gene.goAnnotation.goEvidenceCodes" type="GOEvidenceCode">
    </node>
    <node path="Gene.goAnnotation.goEvidenceCodes.annotType" type="String">
      <constraint op="=" value="high-throughput" description="" identifier="" code="C">
      </constraint>
      <constraint op="=" value="manually curated" description="" identifier="" code="D">
      </constraint>
    </node>
    <node path="Gene.goAnnotation.qualifier" type="String">
      <constraint op="!=" value="NOT" description="" identifier="" code="I">
      </constraint>
      <constraint op="IS NULL" value="null" description="" identifier="" code="J">
      </constraint>
    </node>
    <node path="Gene.featureType" type="String">
      <constraint op="=" value="ORF" description="" identifier="" editable="true" code="B">
      </constraint>
    </node>
    <node path="Gene.status" type="String">
      <constraint op="=" value="Active" description="" identifier="" code="E">
      </constraint>
      <constraint op="IS NULL" value="null" description="" identifier="" code="F">
      </constraint>
    </node>
    <node path="Gene.qualifier" type="String">
      <constraint op="!=" value="Dubious" description="" identifier="" code="G">
      </constraint>
      <constraint op="IS NULL" value="null" description="" identifier="" code="H">
      </constraint>
    </node>
  </query>
</template>
<template name="GOTerm_GeneOrganism" title="GO Term name [and children of this term] --&gt;All genes in organism." longDescription="List all genes that are annotated to the specified GO term and to GO terms that are children of that specified GO Term. Wild card queries (such as *ascospore*) are supported. Only manually curated and high-throughput GO annotations are included. Genes include Uncharacterized and Verified ORFs, pseudogenes, transposable element genes, RNAs, and genes Not in Systematic Sequence of S228C." comment="">
  <query name="GOTerm_GeneOrganism" model="genomic" view="Gene.primaryIdentifier Gene.secondaryIdentifier Gene.symbol Gene.goAnnotation.ontologyTerm.parents.name Gene.goAnnotation.ontologyTerm.parents.identifier Gene.goAnnotation.ontologyTerm.name Gene.goAnnotation.ontologyTerm.identifier Gene.goAnnotation.goEvidenceCodes.annotType Gene.qualifier Gene.goAnnotation.ontologyTerm.namespace" longDescription="List all genes that are annotated to the specified GO term and to GO terms that are children of that specified GO Term. Wild card queries (such as *ascospore*) are supported. Only manually curated and high-throughput GO annotations are included. Genes include Uncharacterized and Verified ORFs, pseudogenes, transposable element genes, RNAs, and genes Not in Systematic Sequence of S228C." sortOrder="Gene.primaryIdentifier asc" constraintLogic="A and B and (C or D) and (E or F) and (G or H) and (I or J)">
    <pathDescription pathString="Gene.goAnnotation.ontologyTerm.parents" description="GO annotation">
    </pathDescription>
    <pathDescription pathString="Gene.goAnnotation.ontologyTerm" description="GO annotation &gt; child term">
    </pathDescription>
    <node path="Gene" type="Gene">
    </node>
    <node path="Gene.goAnnotation" type="GOAnnotation">
    </node>
    <node path="Gene.goAnnotation.ontologyTerm" type="GOTerm">
    </node>
    <node path="Gene.goAnnotation.ontologyTerm.parents" type="GOTerm">
    </node>
    <node path="Gene.goAnnotation.ontologyTerm.parents.name" type="String">
      <constraint op="=" value="*ascospore*" description="Show genes annotated with GO term (and any children of this GO term):" identifier="" editable="true" code="A">
      </constraint>
    </node>
    <node path="Gene.goAnnotation.goEvidenceCodes" type="GOEvidenceCode">
    </node>
    <node path="Gene.goAnnotation.goEvidenceCodes.annotType" type="String">
      <constraint op="=" value="high-throughput" description="" identifier="" code="C">
      </constraint>
      <constraint op="=" value="manually curated" description="" identifier="" code="D">
      </constraint>
    </node>
    <node path="Gene.goAnnotation.qualifier" type="String">
      <constraint op="!=" value="NOT" description="" identifier="" code="I">
      </constraint>
      <constraint op="IS NULL" value="null" description="" identifier="" code="J">
      </constraint>
    </node>
    <node path="Gene.organism" type="Organism">
    </node>
    <node path="Gene.organism.name" type="String">
      <constraint op="=" value="Saccharomyces cerevisiae" description="from organism:" identifier="" editable="true" code="B">
      </constraint>
    </node>
    <node path="Gene.status" type="String">
      <constraint op="=" value="Active" description="" identifier="" code="E">
      </constraint>
      <constraint op="IS NULL" value="null" description="" identifier="" code="F">
      </constraint>
    </node>
    <node path="Gene.qualifier" type="String">
      <constraint op="!=" value="Dubious" description="" identifier="" code="G">
      </constraint>
      <constraint op="IS NULL" value="null" description="" identifier="" code="H">
      </constraint>
    </node>
  </query>
</template>
<template name="GOTerm_Genes" title="GO Term --&gt; All genes" longDescription="List all genes annotated to a specified GO Term.  Manually curated, high-throughput, and computational GO annotations are included.  Genes include Uncharacterized and Verified ORFs, pseudogenes, transposable element genes, RNAs, and genes Not in Systematic Sequence of S228C." comment="">
  <query name="GOTerm_Genes" model="genomic" view="Gene.primaryIdentifier Gene.secondaryIdentifier Gene.featureType Gene.symbol Gene.goAnnotation.ontologyTerm.identifier Gene.goAnnotation.ontologyTerm.name Gene.goAnnotation.ontologyTerm.description Gene.name Gene.sgdAlias Gene.qualifier Gene.status Gene.description Gene.goAnnotation.goEvidenceCodes.annotType Gene.goAnnotation.qualifier" longDescription="List all genes annotated to a specified GO Term.  Manually curated, high-throughput, and computational GO annotations are included.  Genes include Uncharacterized and Verified ORFs, pseudogenes, transposable element genes, RNAs, and genes Not in Systematic Sequence of S228C." sortOrder="Gene.symbol asc" constraintLogic="A and (B or C)">
    <node path="Gene" type="Gene">
    </node>
    <node path="Gene.goAnnotation" type="GOAnnotation">
    </node>
    <node path="Gene.goAnnotation.ontologyTerm" type="GOTerm">
    </node>
    <node path="Gene.goAnnotation.ontologyTerm.name" type="String">
      <constraint op="=" value="sporulation" description="" identifier="" editable="true" code="A" extraValue="">
      </constraint>
    </node>
    <node path="Gene.goAnnotation.qualifier" type="String">
      <constraint op="!=" value="NOT" description="" identifier="" code="B">
      </constraint>
      <constraint op="IS NULL" value="null" description="" identifier="" code="C">
      </constraint>
    </node>
  </query>
</template>
<template name="GOname_GOidentifier" title="GO Term name --&gt; GO Term identifier." longDescription="List the GO Term identifier and description for a specified GO Term name.  Wild card queries (such as *ascospore*) are supported." comment="">
  <query name="GOname_GOidentifier" model="genomic" view="GOTerm.name GOTerm.identifier GOTerm.description GOTerm.namespace" longDescription="List the GO Term identifier and description for a specified GO Term name.  Wild card queries (such as *ascospore*) are supported." sortOrder="GOTerm.name asc" constraintLogic="A and (B or C)">
    <node path="GOTerm" type="GOTerm">
    </node>
    <node path="GOTerm.name" type="String">
      <constraint op="=" value="*ascospore*" description="" identifier="" editable="true" code="A">
      </constraint>
    </node>
    <node path="GOTerm.obsolete" type="Boolean">
      <constraint op="=" value="false" description="" identifier="" code="B">
      </constraint>
      <constraint op="IS NULL" value="null" description="" identifier="" code="C">
      </constraint>
    </node>
  </query>
</template>
<template name="GOtermidentifier_name" title="GO Term identifier --&gt; GO Term name." longDescription="List the GO Term names and descriptions for a specified GO Term identifier.  Note that the query must contain either an entire identifier (such as GO:0007350) or the numerical value preceded by a wild card (such as *30437)." comment="">
  <query name="GOtermidentifier_name" model="genomic" view="GOTerm.identifier GOTerm.name GOTerm.description GOTerm.name GOTerm.namespace" longDescription="List the GO Term names and descriptions for a specified GO Term identifier.  Note that the query must contain either an entire identifier (such as GO:0007350) or the numerical value preceded by a wild card (such as *30437)." sortOrder="GOTerm.identifier asc" constraintLogic="A and (B or C)">
    <node path="GOTerm" type="GOTerm">
