    </node>
    <node path="Gene.goAnnotation" type="GOAnnotation">
    </node>
    <node path="Gene.goAnnotation.ontologyTerm" type="OntologyTerm">
    </node>
    <node path="Gene.goAnnotation.ontologyTerm.parents" type="GOSlimTerm">
    </node>
    <node path="Gene.goAnnotation.ontologyTerm.parents.name" type="String">
      <constraint op="=" value="DNA binding" description="" identifier="" editable="true" code="A" extraValue="">
