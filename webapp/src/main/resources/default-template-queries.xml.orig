    </node>
    <node path="Gene.goAnnotation.ontologyTerm.ontologyAnnotations" type="OntologyAnnotation">
    </node>
    <node path="Gene.goAnnotation.goEvidenceCodes" type="GOEvidenceCode">
    </node>
    <node path="Gene.goAnnotation.goEvidenceCodes.annotType" type="String">
      <constraint op="=" value="high-throughput" description="" identifier="" code="B">
      </constraint>
      <constraint op="=" value="manually curated" description="" identifier="" code="C">
      </constraint>
    </node>
    <node path="Gene.goAnnotation.qualifier" type="String">
      <constraint op="!=" value="NOT" description="" identifier="" code="D">
      </constraint>
      <constraint op="IS NULL" value="null" description="" identifier="" code="E">
      </constraint>
    </node>
    <node path="Gene.status" type="String">
      <constraint op="=" value="Active" description="" identifier="" code="F">
      </constraint>
      <constraint op="IS NULL" value="null" description="" identifier="" code="G">
      </constraint>
    </node>
    <node path="Gene.qualifier" type="String">
      <constraint op="!=" value="Dubious" description="" identifier="" code="H">
      </constraint>
      <constraint op="IS NULL" value="null" description="" identifier="" code="J">
      </constraint>
